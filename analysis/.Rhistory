# create filter based on sd from mean difference score
filter_ss <- function (diff_score, stdev_diff, mean_diff) {
if (diff_score >= mean_diff + (2*stdev_diff) | diff_score <= mean_diff - (2*stdev_diff)) {
remove <- "yes"
} else {
remove <- "no"
}
remove
}
